
//square pattern 6

A B C D E 
A B C D E 
A B C D E 
A B C D E 
A B C D E 


#include<iostream>
using namespace std;
int main(){

    cout<<"Enter the number"<<endl;
    int n;
    cin>>n;

    int i=1;
    //int count=1;
    while(i<=n){
        int j=1;
        //count=i;
        char c='A';
        while(j<=n){
            //char c='A' + i -1;
            cout<<c<<" ";
            //count=count+1;
            c=c+1;
            j=j+1;
        }
        cout<<endl;
        i=i+1;
    }
    return 0;
}
