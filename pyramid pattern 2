
//pyramid pattern 2

1
22
333
4444
55555
666666


#include<iostream>
using namespace std;
int main(){

    cout<<"Enter the number"<<endl;
    int n;
    cin>>n;

    int i=1;
    //int count=1;
    while(i<=n){
        int j=1;
        
        while(j<=i){
            cout<<i;
            //count=count+1;
            j=j+1;
        }
        cout<<endl;
        i=i+1;
    }
    return 0;
}
