//square pattern 5

A A A A A 
B B B B B 
C C C C C 
D D D D D 
E E E E E 


#include<iostream>
using namespace std;
int main(){

    cout<<"Enter the number"<<endl;
    int n;
    cin>>n;

    int i=1;
    //int count=1;
    while(i<=n){
        int j=1;
        //count=i;
        while(j<=n){
            char c='A' + i -1;
            cout<<c<<" ";
            //count=count+1;
            j=j+1;
        }
        cout<<endl;
        i=i+1;
    }
    return 0;
}
